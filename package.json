{
  "name": "@cpangular/ng-material-theme",
  "version": "18.0.0-dev.6",
  "private": true,
  "repository": "https://github.com/cpangular/ng-material-theme",
  "scripts": {
    "prepare": "run-s prepare:*",
    "prepare:husky": "husky",
    "prepare:git": "git config pull.rebase false",
    "recursive": "pnpm run --if-present --recursive --stream --workspace-concurrency=1",
    "recursive-p": "pnpm run --if-present --recursive --stream --parallel",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier --write --ignore-unknown .",
    "fix:git.tags.remove": "pnpm run git.tags.remove",
    "fix:syncpack": "syncpack fix-mismatches",
    "pre-commit": "run-s pre-commit:*",
    "pre-commit:git.tags.remove": "pnpm run git.tags.remove",
    "git.tags.remove": "git tag | xargs git tag -d",
    "git.clean": "git clean -dfX && pnpm i --force",
    "git.reset": "git clean -dfx && git reset --hard && pnpm i --force",
    "build": "pnpm run recursive build",
    "build.watch": "pnpm run recursive-p watch",
    "set-version": "run-p \"set-version:* {1}\" --",
    "set-version:set-version": "pnpm version --allow-same-version --commit-hooks=false --git-tag-version=false",
    "set-version:set-project-versions": "pnpm -r run set-version",
    "set-version-placeholders": "pnpm -r run set-version 0.0.0-PLACEHOLDER",
    "pack": "pnpm run recursive pack",
    "release": "pnpm run recursive release",
    "test": "pnpm  run recursive test",
    "actions-build": "pnpm run build",
    "actions-pack": "pnpm run pack",
    "actions-publish": "pnpm run release",
    "actions-test": "pnpm run test",
    "actions-set-version": "pnpm run set-version"
  },
  "dependencies": {
