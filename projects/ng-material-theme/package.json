{
  "name": "@cpangular/ng-material-theme",
  "version": "0.0.0-PLACEHOLDER",
  "typings": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "scripts": {
    "build": "run-s build:*",
    "build:ts": "tsc -p tsconfig.json",
    "build:copy-package": "ts-node .scripts/copy-package.ts",
    "build:copy-material-theme": "ts-node .scripts/copy-material-theme.ts",
    "start": "tsc -w -p tsconfig.json"
  },
  "repository": "https://github.com/cpangular/ng-material-theme",
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "engines": {
    "node": ">=16.16"
  },
  "devDependencies": {
    "@cpdevtools/lib-node-utilities": "0.4.49",
    "@types/node": "^18.11.19",
    "type-fest": "^3.5.6",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "camel-case": "^4.1.2",
    "linq": "^4.0.1",
    "rxjs": "~7.8.0"
  },
  "exports": {
    ".": {
      "style": "./dist/mat-theme.css",
      "sass": "./src/_index.scss",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./mat-theme.css": {
      "style": "./dist/mat-theme.css"
    },
    "./theming": {
      "sass": "./src/_index.scss"
    },
    "./palettes": {
      "sass": "./src/_palettes.scss"
    }
  }
}
