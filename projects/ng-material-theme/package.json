{
  "name": "@cpangular/ng-material-theme",
  "version": "0.0.0-PLACEHOLDER",
  "typings": "./dist/lib/index.d.ts",
  "main": "./dist/lib/main.js",
  "scripts": {
    "build": "run-s build:*",
    "build:webpack": "webpack",
    "build:css": "sass --load-path=node_modules --style=expanded ./dist/theme_base:./dist/theme",
    "build:npm-ignore": "copyfiles .npmignore ./dist",
    "build:copy-package": "ts-node .scripts/copy-package.ts",
    "set-version": "pnpm version --allow-same-version --commit-hooks=false --git-tag-version=false",
    "start": "tsc -w -p tsconfig.json",
    "pack": "cd dist && mkdir ./pack && pnpm pack --pack-destination ./pack",
    "release": "pnpm publish ./dist/pack/*.tgz --access public --no-git-checks"
  },
  "repository": "https://github.com/cpangular/ng-material-theme",
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "engines": {
    "node": ">=16.16"
  },
  "devDependencies": {
    "@cpdevtools/lib-node-utilities": "0.4.49",
    "@types/node": "^18.13.0",
    "@types/webpack-node-externals": "^3.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "copyfiles": "^2.4.1",
    "file-loader": "^6.2.0",
    "raw-loader": "^4.0.2",
    "sass": "^1.58.0",
    "sass-loader": "^13.2.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "type-fest": "^3.5.6",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "camel-case": "^4.1.2",
    "linq": "^4.0.1",
    "rxjs": "~7.8.0"
  },
  "exports": {
    ".": {
      "style": "./dist/theme/theme-all.css",
      "sass": "./dist/scss/theming/_index.scss",
      "types": "./dist/lib/index.d.ts",
      "default": "./dist/lib/main.js"
    },
    "./theming": {
      "sass": "./dist/scss/theming/_index.scss"
    },
    "./palettes": {
      "sass": "./dist/scss/palettes/_index.scss"
    },
    "./theme.css": "./dist/theme/theme-all.css",
    "./theme/*.css": "./dist/theme/*.css"
  }
}
