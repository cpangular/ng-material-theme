{
  "name": "@cpangular/ng-material-theme",
  "version": "17.3.0-dev.14",
  "scripts": {
    "build": "run-s build:*",
    "build:css": "tsx --tsconfig tsconfig.json src/build-css.mts",
    "xbuild:css": "node --experimental-specifier-resolution=node --experimental-modules --no-warnings --loader ts-node/esm src/build-css.mts",
    "build:ng": "ng build ng-material-theme",
    "watch": "ng build --watch ng-material-theme",
    "set-version": "pnpm version --allow-same-version --commit-hooks=false --git-tag-version=false",
    "pack": "cd ../../ && mkdir ./pack && cd ./dist/ng-material-theme && pnpm pack --pack-destination ../../pack",
    "release": "pnpm publish ../../pack/*.tgz --access public --no-git-checks"
  },
  "peerDependencies": {
    "@angular/cdk": "^18.0.0",
    "@angular/common": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@angular/material": "^18.0.0"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "sideEffects": false,
  "repository": "https://github.com/cpangular/ng-material-theme",
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "devDependencies": {
    "@angular/cdk": "^18.0.0",
    "@angular/material": "^18.0.0",
    "@types/css-tree": "^2.3.7",
    "@types/node": "^20.11.30",
    "colortranslator": "^4.1.0",
    "css-tree": "^2.3.1",
    "prettier": "^3.2.5",
    "sass": "^1.72.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1"
  }
}
