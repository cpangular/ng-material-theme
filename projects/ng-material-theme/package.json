{
  "name": "@cpangular/ng-material-theme",
  "version": "0.0.0-PLACEHOLDER",
  "typings": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "scripts": {
    "build": "run-s build:*",
    "build:ts": "tsc -p tsconfig.json",
    "build:copy-scss": "copyfiles --up=1  \"src/**/*.scss\" dist/",
    "build:copy-ng-scss": "copyfiles --up=3  \"../ng-material-theme-converter/dist/**/_*.scss\" dist/scss/theme",
    "xbuild:copy-package": "ts-node .scripts/copy-package.ts",
    "xbuild:copy-material-theme": "ts-node .scripts/copy-material-theme.ts",
    "set-version": "pnpm version --allow-same-version --commit-hooks=false --git-tag-version=false",
    "start": "tsc -w -p tsconfig.json",
    "pack": "cd dist && mkdir ./pack && pnpm pack --pack-destination ./pack",
    "release": "pnpm publish ./dist/pack/*.tgz --access public --no-git-checks"
  },
  "repository": "https://github.com/cpangular/ng-material-theme",
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "engines": {
    "node": ">=16.16"
  },
  "devDependencies": {
    "@cpdevtools/lib-node-utilities": "0.4.49",
    "@types/node": "^18.13.0",
    "copyfiles": "^2.4.1",
    "ts-node": "^10.9.1",
    "type-fest": "^3.5.6",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "camel-case": "^4.1.2",
    "linq": "^4.0.1",
    "rxjs": "~7.8.0"
  },
  "exports": {
    ".": {
      "style": "./dist/css/ng-material-theme.css",
      "sass": "./src/_index.scss",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./ng-material-theme.css": {
      "style": "./dist/css/ng-material-theme.css"
    },
    "./theming": {
      "sass": "./src/_index.scss"
    },
    "./palettes": {
      "sass": "./src/_palettes.scss"
    }
  }
}
